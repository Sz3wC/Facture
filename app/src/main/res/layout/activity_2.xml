<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity2">

    <TextView
        android:id="@+id/text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Stacja"
        android:layout_centerHorizontal="true"
        />

    <EditText
        android:id="@+id/stationName2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/text1"
        android:text="Orlen" />
    <TextView
        android:id="@+id/text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nr faktury"
        android:layout_below="@id/stationName2"
        android:layout_centerHorizontal="true"
        />
    <EditText
        android:id="@+id/factureName2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text2"
        android:layout_centerHorizontal="true"
        android:text="Orlen" />

    <TextView
        android:id="@+id/text9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Faktura za"
        android:layout_below="@id/factureName2"
        android:layout_centerHorizontal="true"
        />

    <EditText
        android:id="@+id/invoiceFor2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text9"
        android:layout_centerHorizontal="true"
        android:text="Orlen" />


    <TextView
        android:id="@+id/text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Brutto"
        android:layout_below="@id/invoiceFor2"
        android:layout_alignParentRight="true"
        />
    <EditText
        android:id="@+id/sumValue2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/text3"
        android:text="Orlen" />
    <TextView
        android:id="@+id/text7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="VAT"
        android:layout_below="@id/invoiceFor2"
        android:layout_centerHorizontal="true"
        />
    <EditText
        android:id="@+id/VatValue2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/text3"
        android:text="Orlen" />
    <TextView
        android:id="@+id/text8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Netto"
        android:layout_below="@id/invoiceFor2"
        android:layout_alignParentLeft="true"
        />
    <EditText
        android:id="@+id/NettoValue2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/text3"
        android:text="Orlen" />
    <TextView
        android:id="@+id/text4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Firma"
        android:layout_below="@id/sumValue2"
        android:layout_centerHorizontal="true"
        />

    <EditText
        android:id="@+id/companyName2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/text4"
        android:text="Orlen" />
    <TextView
        android:id="@+id/text5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NIP"
        android:layout_below="@id/companyName2"
        android:layout_centerHorizontal="true"
        />
    <EditText
        android:id="@+id/NIP2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/text5"
        android:text="Orlen" />
    <TextView
        android:id="@+id/text6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Data/Nr wydruku"
        android:layout_below="@id/NIP2"
        android:layout_centerHorizontal="true"
        />
    <EditText
        android:id="@+id/date2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/text6"
        android:text="Orlen" />
    <Button
        android:id="@+id/saveToFile2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/date2"
        android:text="EXPORT TO EXCEL"
        />



</RelativeLayout>